{
  "name": "cloudtrail",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:image": "docker build -t spotlight/cloudtrail-dev .",
    "build:tag": "docker tag spotlight/cloudtrail-dev 334678299258.dkr.ecr.us-east-1.amazonaws.com/spotlight/cloudtrail-dev:latest",
    "build:login": "aws ecr get-login-password --region us-east-1 | docker login --username AWS --password-stdin 334678299258.dkr.ecr.us-east-1.amazonaws.com",
    "build:push": "docker push 334678299258.dkr.ecr.us-east-1.amazonaws.com/spotlight/cloudtrail-dev:latest",
    "bulid:deploy1": "aws lambda update-function-code --function-name spotlight-cloudtrail-process-dev --image-uri 334678299258.dkr.ecr.us-east-1.amazonaws.com/spotlight/cloudtrail-dev:latest",
    "bulid:deploy2": "aws lambda update-function-code --function-name spotlight-report-dev --image-uri 334678299258.dkr.ecr.us-east-1.amazonaws.com/spotlight/cloudtrail-dev:latest",
    "prebuild": "rm -rf dist && export AWS_REGION=us-east-1",
    "postbuild": "npm run build:image && npm run build:tag && npm run build:login && npm run build:push && npm run bulid:deploy1 && npm run bulid:deploy2",
    "debug": "ts-node -r tsconfig-paths/register -r dotenv/config src/index.ts dotenv_config_path=.env.dev",
    "test": "jest --detectOpenHandles --forceExit --runInBand",
    "test:local": "jest test/services/iam/iam.test.ts --forceExit --passWithNoTests",
    "start": "node dist/index.js",
    "init:events": "ts-node -r tsconfig-paths/register -r dotenv/config test/initialize/index.ts dotenv_config_path=.env.dev",
    "report": "ts-node -r tsconfig-paths/register -r dotenv/config src/apps/reports.ts dotenv_config_path=.env.dev",
    "imp": "ts-node -r tsconfig-paths/register -r dotenv/config test/import.ts dotenv_config_path=.env.dev",
    "extend": "ts-node -r tsconfig-paths/register -r dotenv/config test/extend.ts dotenv_config_path=.env.dev",
    "truncate": "ts-node -r tsconfig-paths/register -r dotenv/config test/truncate.ts dotenv_config_path=.env.dev",
    "unpro": "ts-node -r tsconfig-paths/register -r dotenv/config test/unprocess.ts dotenv_config_path=.env.dev",
    "reset": "npm run truncate && npm run patch && npm run imp",
    "patch": "ts-node -r tsconfig-paths/register -r dotenv/config test/patch.ts dotenv_config_path=.env.dev",
    "reload:prod": "ts-node -r tsconfig-paths/register -r dotenv/config test/utils/reload.ts dotenv_config_path=.env.prod"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.2",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "json2csv": "^6.0.0-alpha.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@alphax/dynamodb": "^1.2.16",
    "@aws-sdk/client-amplify": "^3.787.0",
    "@aws-sdk/client-api-gateway": "^3.787.0",
    "@aws-sdk/client-apigatewayv2": "^3.808.0",
    "@aws-sdk/client-apprunner": "^3.787.0",
    "@aws-sdk/client-appstream": "^3.808.0",
    "@aws-sdk/client-auto-scaling": "^3.791.0",
    "@aws-sdk/client-backup": "^3.787.0",
    "@aws-sdk/client-cloud9": "^3.808.0",
    "@aws-sdk/client-cloudformation": "^3.787.0",
    "@aws-sdk/client-cloudfront": "^3.787.0",
    "@aws-sdk/client-codebuild": "^3.787.0",
    "@aws-sdk/client-codecommit": "^3.787.0",
    "@aws-sdk/client-codedeploy": "^3.787.0",
    "@aws-sdk/client-cognito-identity": "^3.787.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.787.0",
    "@aws-sdk/client-connect": "^3.791.0",
    "@aws-sdk/client-directory-service": "^3.787.0",
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/client-ec2": "^3.808.0",
    "@aws-sdk/client-ecr": "^3.787.0",
    "@aws-sdk/client-ecs": "^3.791.0",
    "@aws-sdk/client-efs": "^3.787.0",
    "@aws-sdk/client-elastic-beanstalk": "^3.787.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.787.0",
    "@aws-sdk/client-elasticsearch-service": "^3.787.0",
    "@aws-sdk/client-firehose": "^3.793.0",
    "@aws-sdk/client-global-accelerator": "^3.808.0",
    "@aws-sdk/client-glue": "^3.787.0",
    "@aws-sdk/client-iam": "^3.787.0",
    "@aws-sdk/client-iot": "^3.787.0",
    "@aws-sdk/client-lambda": "^3.787.0",
    "@aws-sdk/client-lex-models-v2": "^3.787.0",
    "@aws-sdk/client-rds": "^3.787.0",
    "@aws-sdk/client-route-53": "^3.787.0",
    "@aws-sdk/client-route53profiles": "^3.787.0",
    "@aws-sdk/client-s3": "^3.806.0",
    "@aws-sdk/client-sagemaker": "^3.792.0",
    "@aws-sdk/client-scheduler": "^3.787.0",
    "@aws-sdk/client-sfn": "^3.787.0",
    "@aws-sdk/client-sns": "^3.806.0",
    "@aws-sdk/client-sqs": "^3.787.0",
    "@aws-sdk/client-timestream-write": "^3.787.0",
    "@aws-sdk/client-transfer": "^3.787.0",
    "@aws-sdk/lib-dynamodb": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.806.0",
    "csv-parse": "^5.6.0",
    "lodash": "^4.17.21"
  }
}
