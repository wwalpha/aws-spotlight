{
  "name": "user-manager",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./src/index.ts dotenv_config_path=.env.dev",
    "build": "tsc -p tsconfig.build.json",
    "start": "node index.js",
    "test": "jest"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.272.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/lodash": "^4.14.169",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.11",
    "dotenv": "^16.0.0",
    "jest": "^27.0.3",
    "nodemon": "^2.0.7",
    "superagent": "^7.1.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@alphax/dynamodb": "^1.2.13",
    "aws-sdk": "^2.907.0",
    "axios": "^0.26.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "winston": "^3.3.3"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*.ts"
    ],
    "delay": 3,
    "ignore": [
      "node_modules/*"
    ],
    "execMap": {
      "ts": "ts-node -r dotenv/config"
    },
    "events": {
      "crash": "sh -c 'lsof -i :${PORT:-8080} -t | xargs kill'"
    }
  }
}