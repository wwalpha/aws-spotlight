    "jest": "^27.0.4",
    "prettier": "^3.0.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@alphax/dynamodb": "^1.2.15",
    "aws-sdk": "^2.1328.0",
